// https://github.com/Microsoft/TypeScript/wiki/JSX
{
    "compilerOptions": {
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "target": "es2015",
        "module": "commonjs",
        "noImplicitAny": false,
        "outDir": "built",
        "sourceMap": false 
 
    }
}
